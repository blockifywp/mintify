{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "custom": {
      "border": {
        "radius": "12px"
      },
      "boxShadow": {
        "inset": "",
        "x": "0",
        "y": "10px",
        "blur": "30px",
        "spread": "0",
        "color": "var(--wp--preset--color--shadow)",
        "hover": {
          "y": "6px"
        }
      },
      "strong": {
        "fontWeight": "600"
      },
      "surface": {
        "backdropFilter": "blur(6px)",
        "boxShadow": "var(--wp--custom--box-shadow)",
        "borderRadius": "var(--wp--custom--border--radius)",
        "background": "transparent"
      },
      "surfaceOverlay": {
        "background": "var(--wp--preset--color--background)",
        "opacity": "0.5"
      },
      "darkMode": {
        "palette": {
          "base": "#070b1b",
          "contrast": "#ffffff",
          "body": "#c6efff",
          "surface": "#18323d",
          "outline": "#18323d",
          "shadow": "#00fff010"
        },
        "gradients": {
          "dark": "radial-gradient(50% 50% at 50% 0%, var(--wp--preset--color--primary-lighter) 0%, transparent 100%),radial-gradient(75% 75% at 50% 50%, var(--wp--preset--color--base) 0%, var(--wp--preset--color--shadow) 100%)",
          "text": "var(--wp--preset--gradient--primary)"
        }
      }
    },
    "color": {
      "palette": [
        {
          "name": "Primary Dark",
          "slug": "primary-dark",
          "color": "#00d8d8"
        },
        {
          "name": "Primary",
          "slug": "primary",
          "color": "#06ffff"
        },
        {
          "name": "Primary Light",
          "slug": "primary-light",
          "color": "#a1ffff"
        },
        {
          "name": "Primary Lighter",
          "slug": "primary-lighter",
          "color": "#00f3f324"
        },
        {
          "name": "Contrast",
          "slug": "contrast",
          "color": "#0b1127"
        },
        {
          "name": "Heading",
          "slug": "heading",
          "color": "#070a1c"
        },
        {
          "name": "Body",
          "slug": "body",
          "color": "#284962"
        },
        {
          "name": "Neutral",
          "slug": "neutral",
          "color": "#67919b"
        },
        {
          "name": "Surface",
          "slug": "surface",
          "color": "#e0eff1dd"
        },
        {
          "name": "Outline",
          "slug": "outline",
          "color": "#e0eff0"
        },
        {
          "name": "Base",
          "slug": "base",
          "color": "#ffffff"
        },
        {
          "color": "currentColor",
          "name": "Current Color",
          "slug": "currentColor"
        },
        {
          "color": "inherit",
          "name": "Inherit",
          "slug": "inherit"
        },
        {
          "color": "transparent",
          "name": "Transparent",
          "slug": "transparent"
        },
        {
          "name": "Shadow",
          "slug": "shadow",
          "color": "#a6d9d633"
        }
      ],
      "gradients": [
        {
          "slug": "primary",
          "name": "Primary",
          "gradient": "radial-gradient( circle farthest-corner at 10% 20%,  var(--wp--preset--color--primary-light) 11.3%, var(--wp--preset--color--primary) 41.2%, var(--wp--preset--color--primary-dark) 77.8% )"
        },
        {
          "slug": "light",
          "name": "Light",
          "gradient": "radial-gradient(50% 50% at 50% 0%, var(--wp--preset--color--primary-lighter) 0%, transparent 100%)"
        },
        {
          "slug": "dark",
          "name": "Dark",
          "gradient": "radial-gradient(50% 50% at 50% 0%, var(--wp--preset--color--primary-lighter) 0%, transparent 100%),radial-gradient(75% 75% at 50% 50%, var(--wp--preset--color--contrast) 0%, var(--wp--preset--color--heading) 100%)"
        },
        {
          "slug": "text",
          "name": "Text",
          "gradient": "linear-gradient(135deg, var(--wp--preset--color--contrast) 0%, var(--wp--preset--color--body) 33%, var(--wp--preset--color--heading) 66%)"
        },
        {
          "slug": "pie-chart",
          "name": "Pie Chart",
          "gradient": "conic-gradient(var(--wp--preset--color--surface) 0% 35%, var(--wp--preset--color--shadow) 35% 60%, var(--wp--preset--color--base) 60% 100%)"
        },
        {
          "slug": "background-transparent-blur",
          "name": "Background Transparent Blur",
          "gradient": "linear-gradient(0deg, var(--wp--preset--color--base) 0%, transparent 100%)"
        }
      ]
    },
    "layout": {
      "contentSize": "min(calc((100vw - var(--scrollbar-width,0px)) - var(--wp--preset--spacing--sm,2rem)), 800px)",
      "wideSize": "min(calc((100vw - var(--scrollbar-width,0px)) - var(--wp--preset--spacing--sm,2rem)), 1400px)"
    },
    "spacing": {
      "spacingSizes": [
        {
          "name": "XXS",
          "slug": "xxs",
          "size": "calc(var(--wp--preset--font-size--medium,1rem)/2)"
        },
        {
          "name": "XS",
          "slug": "xs",
          "size": "var(--wp--preset--font-size--medium,1rem)"
        },
        {
          "name": "S",
          "slug": "sm",
          "size": "calc(var(--wp--preset--font-size--medium,1rem)*1.5)"
        },
        {
          "name": "M",
          "slug": "md",
          "size": "calc(var(--wp--preset--font-size--medium,1rem)*2)"
        },
        {
          "name": "L",
          "slug": "lg",
          "size": "calc(var(--wp--preset--font-size--medium,1rem)*3)"
        },
        {
          "name": "XL",
          "slug": "xl",
          "size": "calc(var(--wp--preset--font-size--medium,1rem)*4)"
        },
        {
          "name": "XXL",
          "slug": "xxl",
          "size": "calc(var(--wp--preset--font-size--medium,1rem)*6)"
        }
      ]
    },
    "typography": {
      "fontFamilies": [
        {
          "fontFamily": "Inter,var(--wp--preset--font-family--sans-serif)",
          "name": "Inter",
          "slug": "inter",
          "fontFace": [
            {
              "fontFamily": "Inter",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "fontDisplay": "swap",
              "fontWeight": "100 900",
              "src": [
                "file:./assets/fonts/inter.woff2"
              ]
            }
          ]
        },
        {
          "fontFamily": "Flow Block,var(--wp--preset--font-family--sans-serif)",
          "name": "Flow Block",
          "slug": "flow-block",
          "fontFace": [
            {
              "fontFamily": "Flow Block",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "fontDisplay": "swap",
              "fontWeight": "100 900",
              "src": [
                "file:./assets/fonts/flow-block.woff2"
              ]
            }
          ]
        }
      ],
      "fontSizes": [
        {
          "name": "81px",
          "slug": "81",
          "size": "clamp(54px,8.1vw,81px)",
          "fluid": {
            "min": "54px",
            "max": "81px"
          }
        },
        {
          "name": "72px",
          "slug": "72",
          "size": "clamp(51px,8.1vw,72px)",
          "fluid": {
            "min": "51px",
            "max": "72px"
          }
        },
        {
          "name": "60px",
          "slug": "60",
          "size": "clamp(45px,6vw,60px)",
          "fluid": {
            "min": "45px",
            "max": "60px"
          }
        },
        {
          "name": "54px",
          "slug": "54",
          "size": "clamp(42px,6vw,54px)",
          "fluid": {
            "min": "42px",
            "max": "54px"
          }
        },
        {
          "name": "48px",
          "slug": "48",
          "size": "clamp(36px,4.8vw,48px)",
          "fluid": {
            "min": "36px",
            "max": "48px"
          }
        },
        {
          "name": "42px",
          "slug": "42",
          "size": "clamp(27px,4.2vw,42px)",
          "fluid": {
            "min": "36px",
            "max": "42px"
          }
        },
        {
          "name": "36px",
          "slug": "36",
          "size": "clamp(24px,3.6vw,36px)",
          "fluid": {
            "min": "30px",
            "max": "36px"
          }
        },
        {
          "name": "30px",
          "slug": "30",
          "size": "clamp(21px,3vw,30px)",
          "fluid": {
            "min": "24px",
            "max": "30px"
          }
        },
        {
          "name": "24px",
          "slug": "24",
          "size": "clamp(20px,2.4vw,24px)",
          "fluid": {
            "min": "21px",
            "max": "24px"
          }
        },
        {
          "name": "22px",
          "slug": "22",
          "size": "clamp(19px,2vw,22px)",
          "fluid": {
            "min": "19px",
            "max": "22px"
          }
        },
        {
          "name": "20px",
          "slug": "20",
          "size": "clamp(18px,2vw,20px)",
          "fluid": {
            "min": "18px",
            "max": "20px"
          }
        },
        {
          "name": "18px",
          "slug": "18",
          "size": "clamp(16px,1.8vw,18px)",
          "fluid": {
            "min": "17px",
            "max": "18px"
          }
        },
        {
          "name": "16px",
          "slug": "16",
          "size": "clamp(14px,1.6vw,16px)",
          "fluid": {
            "min": "15px",
            "max": "16px"
          }
        },
        {
          "name": "14px",
          "slug": "14",
          "size": "clamp(12px,1.4vw,14px)",
          "fluid": {
            "min": "13px",
            "max": "14px"
          }
        }
      ]
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--base)"
    },
    "typography": {
    },
    "elements": {
      "button": {
        "border": {
          "radius": "999px"
        },
        "color": {
          "text": "var(--wp--preset--color--body)",
          "background": "var(--wp--preset--color--surface)"
        },
        "shadow": "var(--wp--custom--box-shadow)",
        "spacing": {
          "padding": {
            "top": "0.9em",
            "right": "1.3em",
            "bottom": "0.9em",
            "left": "1.3em"
          }
        },
        "typography": {
          "fontSize": "16px"
        }
      },
      "heading": {
        "color": {
          "text": "var(--wp--preset--color--heading)"
        },
        "typography": {
          "fontWeight": "var(--wp--custom--font-weight--medium)",
          "letterSpacing": "-0.0333em",
          "lineHeight": "1.111"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--81)"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--60)"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--48)"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--30)"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--24)"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--20)"
        }
      }
    },
    "blocks": {
      "core/buttons": {
        "spacing": {
          "blockGap": "var(--wp--preset--spacing--xs)"
        }
      },
      "core/code": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--16)"
        }
      },
      "core/post-date": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--16)"
        }
      },
      "core/post-terms": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--16)"
        }
      }
    }
  }
}
