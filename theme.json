{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "customTemplates": [
    {
      "title": "Blank",
      "name": "blank",
      "postTypes": [
        "page",
        "post"
      ]
    },
    {
      "title": "Full Width",
      "name": "full-width",
      "postTypes": [
        "page",
        "post"
      ]
    }
  ],
  "templateParts": [
    {
      "area": "header",
      "name": "header",
      "title": "Header"
    },
    {
      "area": "footer",
      "name": "footer",
      "title": "Footer"
    }
  ],
  "settings": {
    "custom": {
      "deprecatedColors": {
        "primaryDarker": "var(--wp--preset--color--primary-900)",
        "primaryDark": "var(--wp--preset--color--primary-700)",
        "primary": "var(--wp--preset--color--primary-600)",
        "primaryLight": "var(--wp--preset--color--primary-300)",
        "primaryLighter": "var(--wp--preset--color--primary-100)",
        "contrast": "var(--wp--preset--color--neutral-0)",
        "heading": "var(--wp--preset--color--neutral-800)",
        "body": "var(--wp--preset--color--neutral-600)",
        "neutral": "var(--wp--preset--color--neutral-500)",
        "outline": "var(--wp--preset--color--neutral-200)",
        "surface": "var(--wp--preset--color--neutral-100)",
        "base": "var(--wp--preset--color--neutral-0)"
      },
      "border": {
        "color": "var(--wp--preset--color--neutral-100)"
      }
    },
    "color": {
      "customDuotone": true,
      "customGradient": true,
      "defaultGradients": true,
      "defaultPalette": false,
      "link": true,
      "palette": [
        {
          "color": "#ffffff",
          "name": "Neutral 0",
          "slug": "neutral-0"
        },
        {
          "color": "#f8fafc",
          "name": "Neutral 50",
          "slug": "neutral-50"
        },
        {
          "color": "#f1f5f9",
