          "slug": "neutral-100"
        },
        {
          "color": "#e2e8f0",
          "name": "Neutral 200",
          "slug": "neutral-200"
        },
        {
          "color": "#cbd5e1",
          "name": "Neutral 300",
          "slug": "neutral-300"
        },
        {
          "color": "#94a3b8",
          "name": "Neutral 400",
          "slug": "neutral-400"
        },
        {
          "color": "#64748b",
          "name": "Neutral 500",
          "slug": "neutral-500"
        },
        {
          "color": "#475569",
          "name": "Neutral 600",
          "slug": "neutral-600"
        },
        {
          "color": "#334155",
          "name": "Neutral 700",
          "slug": "neutral-700"
        },
        {
          "color": "#1e293b",
          "name": "Neutral 800",
          "slug": "neutral-800"
        },
        {
          "color": "#0f172a",
          "name": "Neutral 900",
          "slug": "neutral-900"
        },
        {
          "color": "#020617",
          "name": "Neutral 950",
          "slug": "neutral-950"
        },
        {
          "name": "Primary 100",
          "slug": "primary-100",
          "color": "#ecfeff"
        },
        {
          "color": "#67e8f9",
          "name": "Primary 300",
          "slug": "primary-300"
        },
        {
          "color": "#06b6d4",
          "name": "Primary 500",
          "slug": "primary-500"
        },
        {
          "color": "#0891b2",
          "name": "Primary 600",
          "slug": "primary-600"
        },
        {
          "color": "#0e7490",
          "name": "Primary 700",
