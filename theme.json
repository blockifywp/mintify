        },
        {
          "name": "Success 100",
          "slug": "success-100",
          "color": "#d1fae5"
        },
        {
          "name": "Success 500",
          "slug": "success-500",
          "color": "#10b981"
        },
        {
          "name": "Success 600",
          "slug": "success-600",
          "color": "#059669"
        },
        {
          "name": "Warning 100",
          "slug": "warning-100",
          "color": "#feeac7"
        },
        {
          "name": "Warning 500",
          "slug": "warning-500",
          "color": "#f59e0b"
        },
        {
          "name": "Warning 600",
          "slug": "warning-600",
          "color": "#d97706"
        },
        {
          "name": "Error 100",
          "slug": "error-100",
          "color": "#ffe4e6"
